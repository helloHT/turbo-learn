{
  "compilerOptions": {
    "target": "es5",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "moduleResolution": "node",
    "preserveWatchOutput": true,
    "skipLibCheck": true,
    "strict": true,
    "sourceMap": false,
    "declaration": true,
    "outDir": "./dist"
  },
  "include": ["*.ts"],
  "exclude": ["dist", "node_modules", "test"]
}
